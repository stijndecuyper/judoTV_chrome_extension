{
    "manifest_version": 3,
    "name": "judotv plus",
    "version": "1.0",
    "permissions": ["scripting", "activeTab"],
    "action": {
      "default_popup": "hello.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
        {
          "matches": ["https://judotv.com/*"],
          "js": ["content.js"],
          "run_at": "document_end"
        }
      ]
  }  