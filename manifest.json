{
    "manifest_version": 3,
    "name": "judotv bronze",
    "version": "1.0",
    "permissions": ["scripting", "activeTab"],
    "action": {
      "default_popup": "hello.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
        {
          "matches": ["https://judotv.com/*"],
          "js": ["content.js"],
          "run_at": "document_end"
        }
      ],
    "icons": {
       "16": "icon.png",
       "32": "icon.png",
       "48": "icon.png",
       "128": "icon.png"
     }
  }  
